// vers√£o top-down com memos

int [] memo = new int [0];
int fibonacci (int n) {
    if(n<=1) return 1;
    if(n==2) return 2;
    if(memo.length <= n){
        memo[0] = memo[1] = 1;
    }
    return fibrec(n);
}
int fibrec(int n){
    if(n>0){
        if(memo[n]>0) return (memo[n]);
        else {
            memo[n-1] = fibrec(n-1);
            memo[n-2] = fibrec(n-2);
            return(memo[n-1]+memo[n-2]);
        }
    }
    else return 1;
}
